{
	"name": "algorithm-visualizing-3d",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"serve": "vite preview",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,css,less,md,tsx,json,ts}": "prettier --write"
	},
	"dependencies": {
		"react": "^17.0.0",
		"react-dom": "^17.0.0"
	},
	"devDependencies": {
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@vitejs/plugin-react": "^1.0.0",
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.2",
		"prettier": "^2.5.1",
		"typescript": "^4.3.2",
		"vite": "^2.6.4"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
